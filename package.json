{
  "name": "@ximple/igo2",
  "version": "0.19.10-1",
  "description": "IGO Library",
  "main": "bundle/igo.js",
  "typings": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XimpleTech/igo2-lib.git"
  },
  "keywords": [
    "igo"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XimpleTech/igo2-lib.git/issues"
  },
  "homepage": "https://github.com/XimpleTech/igo2-lib.git#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project demo --proxy-config proxy.conf.json",
    "start.lib": "ng serve --proxy-config proxy.conf.json",
    "build.lib": "npm run clean && gulp && ngc -p ./src/tsconfig.lib.json && npm run build.lib.bundle",
    "build.lib.bundle": "webpack --config webpack.conf.js  --bail --display-modules",
    "build.demo": "ng build --project demo",
    "build.demo.prod": "ng build --project demo --prod",
    "test": "npm run lint && ng test --single-run",
    "lint": "ng lint",
    "pree2e": "webdriver-manager clean && webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e --project demo",
    "clean": "rimraf dist && rimraf bundle",
    "language.extract": "npm run language.extract.lib && npm run language.extract.demo",
    "language.extract.lib": "ngx-translate-extract -i ./src/lib -o ./src/locale/fr.json ./src/locale/en.json ./src/locale/zh_TW.json -s -f namespaced-json -m _ -c",
    "language.extract.demo": "ngx-translate-extract -i ./src/demo-app -o ./src/demo-app/assets/locale/fr.json ./src/demo-app/assets/locale/en.json ./src/demo-app/assets/locale/zh_TW.json -s -f namespaced-json -m _ -c",
    "preversion": "npm test && npm run build.demo.prod && npm run build.lib",
    "postversion": "git push && git push --tags && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.4.2",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/material": "6.4.2",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@auth0/angular-jwt": "^2.0.0",
    "@mat-datetimepicker/core": "^2.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular2-notifications": "^1.0.2",
    "bowser": "^1.9.4",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "jspdf": "^1.4.1",
    "jwt-decode": "^2.2.0",
    "ol": "^4.6.5",
    "openlayers": "^4.6.5",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "ts-md5": "^1.2.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.1.1",
    "@angular/compiler-cli": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@angular/platform-server": "6.1.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/geojson": "^7946.0.4",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/jspdf": "^1.1.31",
    "@types/node": "^10.5.5",
    "@types/openlayers": "^4.6.10",
    "@types/yargs": "^10.0.2",
    "ajv": "^6.0.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.5.0",
    "codelyzer": "^4.4.2",
    "css-loader": "^0.28.11",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.6.1",
    "gulp-stylus": "^2.7.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "raw-loader": "^0.5.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "to-string-loader": "^1.1.5",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.16.3",
    "webpack-merge": "^4.1.3"
  },
  "author": "MSP"
}
